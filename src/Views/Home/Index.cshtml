    </div>
    <div class="col-12 col-md-6 col-xl-3 align-self-center form-group">
        <!-- Update branch -->
        <label>Update Branch</label>
        <select class="systemSetting form-control" data-object="SystemData" data-property="UpdateBranch">
            <option value="main">Main</option>
            <option value="dev">Dev</option>
        </select>
    </div>
    <!-- Theme -->
    <div class="col-12 col-md-6 col-xl-3 mt-2 mb-2 align-self-center form-group">
        <label class="form-label">UI Theme</label>
        <select class="form-control" data-object="SystemData" data-property="Theme">
            <option value="light">Light</option>
            <option value="dark">Dark</option>
        </select>
    </div>
    <!-- Auto-update -->
    <div class="col-12 col-md-6 col-xl-3 mt-2 mb-2 align-self-center form-check">
        <label class="form-check-label">Automatically Install Updates</label>
        <input type="checkbox" class="form-check-input" data-object="SystemData" data-property="AutoUpdate"/>
        <div class="form-text">Updates will be installed at 1:00AM every day when enabled.</div>
    </div>
    <!-- Auto-update -->
    <div class="col-12 col-md-6 col-xl-3 mt-2 mb-2 align-self-center form-check">
        <label class="form-check-label">Automatically Disable Streaming</label>
        <input type="checkbox" class="form-check-input" data-object="SystemData" data-property="EnableAutoDisable"/>
        <div class="form-text">Automatically disable control of lights when no input is detected.</div>
    </div>
</div>

<!-- Capture Mode -->
<div class="row border justify-content-center pb-5">
    <div class="col-12 headerCol">Capture Preferences</div>
    <div class="col-12 col-md-6 col-xl-3">
        <label class="form-label">Capture Mode</label>
        <select class="form-control" data-object="SystemData" data-property="CaptureMode">
            <option value="0">DreamScreen</option>
            <option value="1">Camera</option>
            <option value="2">HDMI</option>
            <option value="3">Screen Capture</option>
        </select>
        <div class="form-text">Select your desired input source.</div>
    </div>
    <div class="col-12 col-md-6 col-xl-3 capGroup form-group" id="UsbSelectRow">
        <label class="form-label">USB Selection</label>
        <select class="form-control" data-object="SystemData" data-property="UsbSelection" id="UsbSelect">
            <option value=""></option>
        </select>
    </div>
    <div class="col-12 col-md-6 col-xl-3 capGroup form-group" data-mode="0">
        <label class="form-label">Target DreamScreen</label>
        <select id="targetDs" class="form-control" data-object="SystemData" data-property="DsIp">
            <option value=""></option>
        </select>
    </div>
    <div class="col-12 col-md-6 col-xl-3 capGroup" data-mode="1">
        <label class="form-label">Camera Type</label>
        <select class="form-control" data-object="SystemData" data-property="CamType">
            <option value="0">Raspberry Pi Camera</option>
            <option value="1">USB Web Camera</option>
        </select>
    </div>
    
    <div class="col-12 col-md-6 col-xl-3 capGroup" data-mode="3" id="CapModeSelectRow">
        <label class="form-label">Screen Capture Mode</label>
        <select class="form-select" data-object="SystemData"
                data-property="ScreenCapMode" id="ScreenCapMode">
            <option value="0">Region</option>
            <option value="1">Monitor</option>
        </select>
        <div class="form-text">Select the screen capture mode.</div>
    </div>
    
    <div class="col-12 col-md-6 col-xl-3 capGroup" data-mode="3" id="MonitorSelectRow">
        <label class="form-label">Monitor Selection</label>
        <select class="form-select" multiple aria-label="multimonitor" data-object="SystemData"
                data-property="SelectedMonitors" id="monitorSelect">
        </select>
        <div class="form-text">Select the target monitor(s) to capture.</div>
    </div>
    
    <div class="col-12 col-md-9 capGroup" data-mode="3" id="RegionSelectRow">
            <label class="form-label">Region Selection</label>
        <div id="RegionSelect">
            <img class="img-fluid" src="/img/_preview_screen.jpg" alt="Screen Preview" id="croppr"></img>
        </div>
            <div class="form-text">Select the target region to capture.</div>
        </div>
    
    <div class="col-12 col-md-6 col-xl-3 capGroup" data-mode="2">

    </div>
    
</div>
<!-- Capture Preview -->
<div class="row border justify-content-center pb-5">
    <div class="col-12 headerCol">Preview</div>
    <div class="col-6">
        <div class="col-12 titleCol">Input</div>
        <div class="col-12">
            <img id="inputPreview" class="prevImg img-fluid" alt="Input preview." src="~/img/no_preview.jpg"/>
        </div>
    </div>
    <div class="col-6">
        <div class="col-12 titleCol">Output</div>
        <div class="col-12">
            <img id="outputPreview" class="prevImg img-fluid" alt="Output preview." src="~/img/no_preview.jpg"/>
        </div>
    </div>
    <div class="col-12 col-md-6 col-xl-3 form-group">
        <label class="form-label">Preview Mode</label>
        <select data-object="SystemData" data-property="PreviewMode" class="form-control">
            <option value="0">None</option>
            <option value="1">LED</option>
            <option value="2">Sectors</option>
        </select>
    </div>
</div>

<div class="row border justify-content-center pb-5">
    <div class="col-12 titleCol">LED Counts</div>
    <div class="col-12 form-text">Enter the total number of LEDs around the perimerter of your screen.</div>
    <!-- Top count -->
    <div class="col-5"></div>
    <div class="col-2 align-self-center">
        <div class="input-group mb-3">
            <div class="input-group-prepend">
                <span class="input-group-text" id="ledCountTop">Top</span>
            </div>
            <input type="text" class="form-control ledCount" data-property="TopCount" data-object="SystemData" aria-label="Top" aria-describedby="ledCountTop">
        </div>
    </div>
    <div class="col-5"></div>
    <!-- Left count -->
    <div class="col-3">
        <div class="input-group mb-3">
            <div class="input-group-prepend">
                <span class="input-group-text" id="ledCountLeft">Left</span>
            </div>
            <input type="text" class="form-control ledCount" data-property="LeftCount" data-object="SystemData" aria-label="Left" aria-describedby="ledCountLeft">
        </div>
    </div>
    <!-- Preview -->
    <div class="col-6" id="sLedPreview">
        <img id="ledImage" class="img-fluid" src="~/img/sectoring_screen.png" alt="ledImage">
    </div>
    <!-- Right count -->
    <div class="col-3">
        <div class="input-group mb-3">
            <div class="input-group-prepend">
                <span class="input-group-text" id="ledCountRight">Right</span>
            </div>
            <input type="text" class="form-control ledCount" data-property="RightCount" data-object="SystemData" aria-label="Right" aria-describedby="ledCountRight">
        </div>
    </div>
    <div class="col-5"></div>
    <!-- Bottom count -->
    <div class="col-2 align-self-center">
        <div class="input-group mb-3">
            <div class="input-group-prepend">
                <span class="input-group-text" id="ledCountBottom">Bottom</span>
            </div>
            <input type="text" class="form-control ledCount" data-property="BottomCount" data-object="SystemData" aria-label="Bottom" aria-describedby="ledCountBottom">
        </div>
    </div>
    <div class="col-5"></div>
</div>

<div class="row border justify-content-center pb-5">
    <div class="col-12 titleCol">Sector Counts</div>
    <div class="col-12 form-text">Enter the number of vertical and horizontal sectors. You will need to re-map your sectors for devices after this.</div>
    <!-- Top count -->
    <div class="col-5"></div>
    <div class="col-2 align-self-center">
        <div class="input-group mb-3">
            <div class="input-group-prepend">
                <span class="input-group-text" id="hSectorCount">Horizontal</span>
            </div>
            <input type="text" class="form-control ledCount" data-property="HSectors" data-object="SystemData" aria-label="Top" aria-describedby="ledCountTop">
        </div>
    </div>
    <div class="col-5"></div>
    <!-- Left count -->
    <div class="col-3">
        <div class="input-group mb-3">
            <div class="input-group-prepend">
                <span class="input-group-text" id="vSectorCount">Vertical</span>
            </div>
            <input type="text" class="form-control ledCount" data-property="VSectors" data-object="SystemData" aria-label="Left" aria-describedby="ledCountLeft">
        </div>
    </div>
    <!-- Preview -->
    <div class="col-6" id="sectorPreview">
        <img id="sectorImage" class="img-fluid" src="~/img/sectoring_screen.png" alt="ledImage">
    </div>
    
</div>
<div class="row border justify-content-center pb-5">
    <div class="col-12 titleCol">OpenRGB</div>
    <div class="col-12 form-text">Enter the IP Address and port of your OpenRGB Server.</div>
    <div class="col-12 col-md-6 col-xl-3 form-group">
        <label class="form-label">IP Address</label>
        <input type="text" class="form-control" data-object="SystemData" data-property="OpenRgbIp" id="OpenRgbIp"/>
    </div>
     <div class="col-12 col-md-6 col-xl-3 form-group">
        <label class="form-label">Port</label>
        <input type="text" class="form-control" data-object="SystemData" data-property="OpenRgbPort" id="OpenRgbPort"/>
     </div>    
</div>
</div>
</div>
</div>
<div class="tab-pane fade" id="audioTab" role="tabpanel" aria-labelledby="audio-tab">
    <div class="container-fluid">
        <div class="row border justify-content-center pb-5">
            <div class="col-12 headerCol">Music Mode</div>
            <div class="col-12 col-md-6 col-xl-3 justify-content-center form-group">
                <label class="form-label">Input Device</label>
                <select class="form-control" data-object="SystemData" data-property="RecDev" id="RecDev">

                </select>
            </div>
        </div>
        <div class="row border justify-content-center pb-5">
            <div class="col-12 col-md-6 col-xl-3 justify-content-center form-group">
                <label class="form-label">Sector Map</label>
                <select class="form-control" data-object="SystemData" data-property="AudioMap">
                    <option value="0">Bottom</option>
                    <option value="1">Top</option>
                    <option value="2">Center</option>
                    <option value="3">Middle</option>
                    <option value="4">Corners</option>
                </select>
                <img class="img-fluid" id="audioMapImg" alt="Audio Map Image" src=""/>
            </div>
            <div class="col-12 col-md-6 col-xl-3 justify-content-center form-group">
                <div class="col-12 justify-content-center form-group">
                    <label class="form-label">Rotation Speed</label>
                    <input class="form-control" data-object="SystemData" data-property="AudioRotationSpeed" type="number" min=".99" max=".99" step=".01"/>
                </div>
                <div class="col-12 justify-content-center form-group">
                    <label class="form-label">Rotation Sensitivity</label>
                    <input class="form-control" data-object="SystemData" data-property="AudioRotationSensitivity" type="number" min="-.99" max=".99" step=".01"/>
                </div>
                <div class="col-12 justify-content-center form-group">
                    <label class="form-label">Rotation Lower Limit</label>
                    <input class="form-control" data-object="SystemData" data-property="AudioRotationLower" type="number" min="0" max="1" step=".01"/>
                </div>
                <div class="col-12 justify-content-center form-group">
                    <label class="form-label">Rotation Upper Limit</label>
                    <input class="form-control" data-object="SystemData" data-property="AudioRotationUpper" type="number" min="0" max="1" step=".01"/>
                </div>
                <div class="col-12 justify-content-center form-group">
                    <label class="form-label">Audio Gain</label>
                    <input class="form-control" data-object="SystemData" data-property="AudioGain" type="number" min="0" max="5" step=".1"/>
                </div>
                <div class="col-12 justify-content-center form-group">
                    <label class="form-label">Low Audio Cutoff</label>
                    <input class="form-control" data-object="SystemData" data-property="AudioMin" type="number" min="0" max=".1" step=".001"/>
                </div>
            </div>
        </div>
    </div>
