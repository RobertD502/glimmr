@{
    Layout = "_Layout";
}

<!DOCTYPE html>

<html lang="en">
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>New Hotness</title>
</head>
<body>
<div class="main">
<nav class="myNav navbar-light bg-light fixed-top" id="mainNav">
    <div class="myNav container-fluid">
        <div class="btn-group btn-group-lg myNav">
            <div class="btn btn-clear modeBtn" data-mode="0" data-toggle="tooltip" data-placement="top" title="Off">
                <span class="material-icons">power_off</span>
            </div>
            <div class="btn btn-clear modeBtn" data-mode="1" data-toggle="tooltip" data-placement="top" title="Video">
                <span class="material-icons">tv</span>
            </div>
            <div class="btn btn-clear modeBtn" data-mode="2" data-toggle="tooltip" data-placement="top" title="Audio">
                <span class="material-icons">music_note</span>
            </div>
            <div class="btn btn-clear modeBtn" data-mode="4" data-toggle="tooltip" data-placement="top" title="AudioVideo">
                <span class="material-icons">music_video</span>
            </div>
            <div class="btn btn-clear modeBtn" data-mode="3" data-toggle="tooltip" data-placement="top" title="Ambient">
                <span class="material-icons">color_lens</span>
            </div>
            <div class="btn btn-clear modeBtn" data-mode="5" data-toggle="tooltip" data-placement="top" title="Streaming">
                <span class="material-icons">sensors</span>
            </div>
        </div>
        <div class="btn-group btn-group-lg myNav float-right">
            <div class="btn btn-clear btn-lg btn-offset mainSettings">
                <span class="material-icons">settings_applications</span>
            </div>
            <div class="btn btn-clear btn-lg btn-offset d-none" id="closeBtn">
                <span class="material-icons">arrow_left</span>
            </div>
        </div>
    </div>

</nav>
<div class="bg-dark hide container-fluid" id="ambientNav">
    <div class="row">
        <div class="col-6 row">
            <div class="col-2">
                <label class="form-label">Ambient Scene</label>    
            </div>
            <div class="col-10">
                <select class="form-control" data-property="AmbientShow" data-object="SystemData" id="AmbientShow">
                    
                </select>    
            </div>
            
        </div>
        <div class="col-6 row">
            <div class="col-2">
                <label class="form-label">
                    Ambient color
                </label>
            </div>
            <div class="col-10">
                <div class="pickrBtn form-control"></div>    
            </div>
        </div>
    </div>
</div>
<div id="mainContent">
<div class="row justify-content-center" id="cardRow">

</div>
<div class="row justify-content-center d-none" id="settingsRow">
<div class="col-12">
<div class="card" id="mainSettingsCard" aria-labelledby="mainSettingsCard" data-backdrop="false" aria-hidden="true">
<div class="card-content">
<div class="card-dialog">
<div class="card-header">
    <h5 class="card-title" id="settingsTitle">Settings</h5>
</div>
<div class="card-body">
<div class="container-fluid">
<ul class="nav nav-tabs" id="settingsTab" role="tablist">
    <li class="nav-item" role="presentation">
        <a class="nav-link active" id="home-tab" data-toggle="tab" href="#mainTab" role="tab" aria-controls="home" aria-selected="true">Main</a>
    </li>
    <li class="nav-item" role="presentation">
        <a class="nav-link" id="profile-tab" data-toggle="tab" href="#audioTab" role="tab" aria-controls="profile" aria-selected="false">Audio</a>
    </li>
    <li class="nav-item" role="presentation">
        <a class="nav-link" id="contact-tab" data-toggle="tab" href="#systemTab" role="tab" aria-controls="contact" aria-selected="false">System</a>
    </li>
</ul>
<div class="tab-content" id="settingsContent">
<div class="tab-pane fade show active" id="mainTab" role="tabpanel" aria-labelledby="main-tab">
<div class="container-fluid">
<!-- System prefs -->
<div class="row border justify-content-center pb-5">
    <div class="col-12 headerCol">General</div>
    <div class="col-12 col-md-6 col-xl-3 justify-content-center form-group">
        <label class="form-label">System Control</label>
        <div class="btn-group form-control">
            <div class="btn btn-lg btn-clear controlBtn" data-action="shutdown" data-bs-toggle="tooltip" data-bs-placement="top" title="Shutdown">
                <span class="material-icons">power_settings_new</span>
            </div>
            <div class="btn btn-lg btn-clear controlBtn" data-action="reboot" data-bs-toggle="tooltip" data-bs-placement="top" title="Reboot">
